{
  "name": "mirv",
  "version": "0.1.2",
  "description": "Clustered Web Server",
  "main": "./dist/lib/index.js",
  "bin": {
    "mirv": "./dist/mirv.js"
  },
  "scripts": {
    "start": "cd dist && NODE_ENV=dev node ./mirv.js | bunyan",
    "prod": "cd dist && ./mirv",
    "prepublish": "eslint ./src/ && npm run test && npm run build",
    "build": "babel src/ -d dist/ && cp package.json dist/",
    "clean": "rm -r ./dist",
    "test": "npm run build && (NODE_ENV=test istanbul cover -x 'dist/spec/*.spec.js' jasmine -- dist/spec/*.spec.js && [ -z ${COVERALLS_REPO_TOKEN} ] || cat ./coverage/lcov.info | ../node_modules/coveralls/bin/coveralls.js)",
    "dev-test": "npm run build && NODE_ENV=test jasmine dist/spec/*.spec.js",
    "coverage": "npm run build && (NODE_ENV=test istanbul cover -x 'dist/spec/*.spec.js' jasmine -- dist/spec/*.spec.js && open ./coverage/lcov-report/index.html)",
    "mega-clean": "rm -r ./node_modules ./dist",
    "publish-beta": "npm publish --tag beta"
  },
  "author": "Henri Bouvier",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-preset-env": "^1.4.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-cli": "^1.1.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "lint": "^1.1.2"
  },
  "dependencies": {
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.16.0",
    "bunyan": "^1.8.5",
    "bunyan-middleware": "^0.6.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-toobusy": "0.0.3",
    "restify": "^4.3.0",
    "restify-bunyan-logger": "^2.0.7",
    "socket.io": "^1.7.3"
  },
  "engines": {
    "node": ">=7.2.0"
  },
  "files": [
    "./dist",
    "./examples"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "engineStrict": true
}
